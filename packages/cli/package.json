{
  "name": "@corex-ui/cli",
  "description": "Corex CLI",
  "version": "0.2.1",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "author": "Corex UI",
  "bin": {
    "corex-ui": "./bin/run.js"
  },
  "dependencies": {
    "@corex-ui/static": "workspace:*",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/corex-ui/corex-ui.git",
    "directory": "packages/cli"
  },
  "homepage": "https://corex-ui.com",
  "bugs": {
    "url": "https://github.com/corex-ui/corex-ui/issues"
  },
  "keywords": [
    "oclif",
    "corex cli"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "corex-ui",
    "dirname": "corex-ui",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "render": {
        "description": "Render Corex UI on static sites"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
