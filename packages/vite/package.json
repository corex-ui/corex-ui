{
  "name": "@corex-ui/vite",
  "version": "0.0.1",
  "description": "Vite plugin for rendering Corex UI static components post-build.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vite-corex-render.cjs.js",
  "module": "./dist/vite-corex-render.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/vite-corex-render.cjs.js",
      "import": "./dist/vite-corex-render.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corex-ui/corex-ui"
  },
  "bugs": {
    "url": "https://github.com/corex-ui/corex-ui/issues"
  },
  "homepage": "https://corex-ui.com",
  "keywords": [
    "vite",
    "vite-plugin",
    "corex",
    "corex-ui",
    "components",
    "static",
    "render"
  ],
  "author": "Netoum <info@netoum.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@zag-js/dismissable": "^1.25.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "intersection-observer": "^0.12.2",
    "resize-observer-polyfill": "^1.5.1"
  }
}
