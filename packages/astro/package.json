{
  "name": "@corex-ui/astro",
  "version": "0.1.8",
  "description": "Astro integration for rendering Corex UI static components post build.",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/corex-ui/corex-ui.git",
    "directory": "packages/astro"
  },
  "homepage": "https://corex-ui.com",
  "bugs": {
    "url": "https://github.com/corex-ui/corex-ui/issues"
  },
  "type": "module",
  "main": "./dist/render.cjs.js",
  "module": "./dist/render.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/render.cjs.js",
      "import": "./dist/render.es.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "prepublishOnly": "npm run build",
    "lint": "eslint"
  },
  "keywords": [
    "astro",
    "astro-integration",
    "corex",
    "corex-ui",
    "components",
    "static",
    "render"
  ],
  "files": [
    "README.md",
    "LICENSE.md",
    "package.json",
    "dist/*"
  ],
  "author": "Netoum <info@netoum.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "typescript": "~5.8.3",
    "vite-plugin-dts": "^4.5.4",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "astro": "^5.13.10",
    "intersection-observer": "^0.12.2",
    "resize-observer-polyfill": "^1.5.1",
    "jsdom": "^26.1.0"
  }
}
